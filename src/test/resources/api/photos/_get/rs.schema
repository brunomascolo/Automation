{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": [
        {
      "type": "object",
          "properties": {
             "albumId": {
               "type": "integer"
             },
             "id": {
               "type": "integer"
              },
              "title": {
                "type": "string"
             },
             "url": {
               "type": "string",
               "format": "uri",
               "qt-uri-protocols": [
                 "https"
              ]
             },
              "thumbnailUrl": {
                "type": "string",
                "format": "uri",
                "qt-uri-protocols": [
                  "https"
               ]
                 }
              },
              "required": [
                "albumId",
                "id",
                "thumbnailUrl",
                "title",
                "url"
                ]
        }

    ]
}